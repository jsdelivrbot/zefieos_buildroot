#!/bin/bash

USBMNT="/usb"
USBPART=1
DESTMNT="/dest"
IMGROOTDIR="${USBMNT}/data"
BLOCKSIZE=4096
TWRP_TAR_FLAGS="--strip-components=1 -xp"
DIAGHEIGHT=24
DIAGWIDTH=80
DISK_TIMEOUT=5
PVF="zos_xfer %B %S %r %a"

LOGFILE="/zefie.log"
USBLOG="${USBMNT}/${LOGFILE}"

KERNEL_SUPPORT_REBOOT_TARGET=0
DORESTORE=0
DOPARTITION=0
DOWIPE=0
ISERROR=0
ISEMMC=0
VERIFYYES=0
NODEVS=0

OSNAME="zefieOS"

set IMGDIR
set TXTPART
set SGDPART
set IMGNAME

